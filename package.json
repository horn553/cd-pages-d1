{
	"name": "cf-pages-d1",
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "run-p -r dev:vite dev:wrangler",
		"dev:vite": "vite dev",
		"dev:wrangler": "wrangler dev node_modules/wrangler-proxy/dist/worker.js",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"gen:wrangler.toml": "envsub --all --env-file .env wrangler.base.toml wrangler.toml",
		"migrate:remote": "dotenv cross-var 'wrangler d1 migrations apply $D1_DATABASE_NAME --remote'",
		"migrate:local": "dotenv cross-var 'wrangler d1 migrations apply $D1_DATABASE_NAME --local'",
		"gen:migration": "drizzle-kit generate:sqlite"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20241022.0",
		"@fontsource/fira-mono": "^5.0.0",
		"@neoconfetti/svelte": "^2.0.0",
		"@sveltejs/adapter-cloudflare": "^0.0.1",
		"@sveltejs/kit": "^0.0.30",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"@types/eslint": "^9.6.0",
		"better-sqlite3": "^11.5.0",
		"cross-var": "^1.1.0",
		"dotenv-cli": "^7.4.2",
		"drizzle-kit": "^0.27.0",
		"envsub": "^4.1.0",
		"eslint": "^9.7.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.36.0",
		"globals": "^15.0.0",
		"npm-run-all": "^4.1.5",
		"prettier": "^3.3.2",
		"prettier-plugin-svelte": "^3.2.6",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.0.0",
		"vite": "^5.0.3",
		"wrangler": "^3.84.1",
		"wrangler-proxy": "^2.2.7"
	},
	"dependencies": {
		"drizzle-orm": "^0.36.0"
	}
}
